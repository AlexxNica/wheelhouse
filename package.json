{
  "name": "wheelhouse",
  "version": "0.0.1",
  "description": "Swiss Army knife for Javascript + Kubernetes",
  "main": "package.json",
  "scripts": {
    "test": "npm run lint && lerna run test",
    "postinstall": "lerna exec --concurrency=1 --no-sort -- yarn install --no-lockfile && lerna bootstrap",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "dev": "DEBUG='wheelhouse:*' lerna exec --concurrency=999 --no-sort yarn run dev"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add",
      "eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/streamplace/wheelhouse.git"
  },
  "keywords": [
    "Kubernetes"
  ],
  "author": "Streamplace",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streamplace/wheelhouse/issues"
  },
  "homepage": "https://github.com/streamplace/wheelhouse#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.13.3",
    "lerna": "2.0.0-beta.37",
    "lint-staged": "^3.4.0"
  },
  "dependencies": {
    "eslint-plugin-prettier": "^2.0.1",
    "milligram": "^1.3.0",
    "prettier": "^1.3.1",
    "update-notifier": "^2.1.0"
  }
}
